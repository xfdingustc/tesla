<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="toolbarColor" format="color"/>
    <attr name="defaultBgColor" format="color"/>
    <attr name="defaultBgColorDark" format="color"/>
    <attr name="hachiTextColorPrimary" format="color"/>
    <attr name="hachiTextColorSecondary" format="color"/>
    <attr name="hachiTextColorDisabled" format="color"/>
    <attr name="defaultDialogBgColor" format="color"/>
    <attr name="appTextColorPrimary" format="color"/>
    <attr name="appTextColorSecondary" format="color"/>
    <attr name="appTextColorDisabled" format="color"/>
    <attr name="actionButtonColor" format="color"/>
    <attr name="signinLogoColor" format="color"/>
    <attr name="separateColor" format="color"/>
    <attr name="momentPlaceHolderColor" format="color"/>
    <attr name="overtureBgColor" format="color"/>
    <attr name="overtureLogoTint" format="color"/>
    <attr name="cardViewBgColor" format="color"/>


    <declare-styleable name="VideoTrimmer">
        <attr name="thumbWidth" format="dimension"/>
        <attr name="progressBarWidth" format="dimension"/>
        <attr name="boardWidth" format="dimension"/>
        <attr name="leftThumb" format="reference"/>
        <attr name="rightThumb" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ClipPlayFragment">
        <attr name="rippleAlpha" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MultiSegSeekbar">
        <attr name="segInactiveColor" format="color"/>
        <attr name="segActiveColor" format="color"/>
        <attr name="dividerWidth" format="dimension"/>
        <attr name="barMinHeight" format="dimension"/>
        <attr name="barPaddingBottom" format="dimension"/>
        <attr name="circleSize" format="dimension"/>
        <attr name="circleColor" format="color"/>
        <attr name="barHeight" format="dimension"/>
        <attr name="progressColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="CompoundEditView">
        <attr name="cevType" format="enum">
            <enum name="email" value="0"/>
            <enum name="password" value="1"/>
            <enum name="verification_code" value="2" />
        </attr>
        <attr name="cevHint" format="string|reference" />
        <attr name="cevInvalidFormatMessage" format="string|reference" />
        <attr name="cevValidPattern" format="string|reference"/>
    </declare-styleable>


    <declare-styleable name="RoundView">

        <attr name="text" format="string"/>
    </declare-styleable>


    <declare-styleable name="RangeBar">
        <attr name="tickStart" format="integer" />
        <attr name="tickEnd" format="integer" />
        <attr name="tickInterval" format="float" />
        <attr name="tickHeight" format="dimension" />
        <attr name="tickColor" format="color" />
        <attr name="barWeight" format="dimension" />
        <attr name="rangeBarColor" format="reference|color" />
        <attr name="textColor" format="reference|color" />
        <attr name="pinColor" format="reference|color" />
        <attr name="selectorSize" format="dimension" />
        <attr name="pinPadding" format="dimension" />
        <attr name="pinMinFont" format="dimension" />
        <attr name="pinMaxFont" format="dimension" />
        <attr name="rangeBarPaddingBottom" format="dimension" />
        <attr name="selectorColor" format="reference|color" />
        <attr name="rangeBar" format="boolean" />
        <attr name="temporaryPins" format="boolean" />
        <attr name="connectingLineWeight" format="dimension" />
        <attr name="connectingLineColor" format="reference|color" />
        <attr name="pinRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="SeekBarPreference">
        <attr name="msbp_minValue" format="reference|integer"/>
        <attr name="msbp_maxValue" format="reference|integer"/>
        <attr name="msbp_interval" format="reference|integer"/>
        <attr name="msbp_measurementUnit" format="reference|string"/>
        <attr name="msbp_dialogEnabled" format="reference|boolean"/>

        <!--next ones should be used only with view!-->
        <attr name="msbp_view_title" format="reference|string"/>
        <attr name="msbp_view_summary" format="reference|string"/>
        <attr name="msbp_view_defaultValue" format="reference|integer"/>
        <attr name="msbp_view_enabled" format="reference|boolean"/>

        <!--not implemented yet-->
        <!--<attr name="msbp_dialogStyle" format="reference"/>-->
    </declare-styleable>

    <declare-styleable name="DropDownMenu">

        <attr name="ddunderlineColor" format="color"/>
        <attr name="dddividerColor" format="color"/>
        <attr name="ddtextSelectedColor" format="color"/>
        <attr name="ddtextUnselectedColor" format="color"/>
        <attr name="ddmenuBackgroundColor" format="color"/>
        <attr name="ddmaskColor" format="color"/>
        <attr name="ddmenuTextSize" format="dimension"/>
        <attr name="ddmenuSelectedIcon" format="reference"/>
        <attr name="ddmenuUnselectedIcon" format="reference"/>
    </declare-styleable>

</resources>
